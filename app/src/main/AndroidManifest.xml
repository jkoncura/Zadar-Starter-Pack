<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neta.android.zadar_starterpack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeachesItemActivity"
            android:label="@string/category_beaches"
            android:parentActivityName=".BeachesItemActivity" />
        <activity
            android:name=".CuisineItemActivity"
            android:label="@string/category_cuisine"
            android:parentActivityName=".CuisineItemActivity" />
        <activity
            android:name=".NightLifeItemActivity"
            android:label="@string/category_nightlife"
            android:parentActivityName=".NightLifeItemActivity"/>

    </application>

</manifest>